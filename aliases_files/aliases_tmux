# tmux
alias tmn='tmux new -s'
alias tmls='tmux ls'
alias tma='tmux a -t'
alias tmks='tmux kill-session -t'

# tmuxinator
alias mux=tmuxinator
alias trn='tmuxinator new'
alias trs='tmuxinator start'
alias trls='tmuxinator list'

function tmnc() {
  DIR="`basename $PWD`"
  CMD="tmux new -s $DIR"
  eval $CMD
}
