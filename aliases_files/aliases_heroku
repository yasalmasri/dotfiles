# Heroku aliases
alias hga='heroku git:remote -a' # to add app to folder.
alias gph='git push heroku master'
alias gphd='git push heroku dev:master'
alias hr='heroku run'
alias hrc='heroku run rails c'
alias hrm='heroku run rake db:migrate'
alias hrs='heroku run rake db:seed'
alias hdbr='heroku pg:reset DATABASE'
alias hl='heroku logs --tail'
alias ho='heroku open'
alias hpsta='git push staging $(branch_name):master'
alias hptst='git push testing $(branch_name):master'
