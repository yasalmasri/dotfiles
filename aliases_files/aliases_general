# General
# alias op='open .'
alias ..='cd ..'
alias ls='ls -1 -F -p'
alias clean='clear; echo -e "\033c\e[3J"'
alias clr='clear'
alias stst='speedtest'
alias setenv='source .env'
alias loenv='export $(grep -v '^#' .env | xargs -0)'
alias gtags='ctags -R --languages=ruby .'
alias nvimdiff='nvim -d'
alias gr='clean && grep --color -inr'
alias notes='cd /Volumes/Files/Notes && nvim'
